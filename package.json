{
  "name": "@ngutil/source",
  "version": "0.0.3-dev.18",
  "license": "MIT",
  "scripts": {
    "before:release:update-packages": "pnpm update && git add . && (git diff-index --quiet HEAD || git commit -m \"chore(tools): Update pnpm-lock.yaml\")",
    "before:release:test": "nx run-many -t test",
    "before:release:format": "nx format:write && git add . && (git diff-index --quiet HEAD || git commit -m \"format: Beautify files\")",
    "before:release": "pnpm before:release:update-packages && pnpm before:release:test && pnpm before:release:format",
    "release": "release-it --ci -V --preRelease=dev",
    "build:libs": "nx run-many -t build --projects=packages/*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.1.3",
    "@angular/common": "~17.1.3",
    "@angular/compiler": "~17.1.3",
    "@angular/core": "~17.1.3",
    "@angular/forms": "~17.1.3",
    "@angular/platform-browser": "~17.1.3",
    "@angular/platform-browser-dynamic": "~17.1.3",
    "@angular/router": "~17.1.3",
    "@nx/angular": "18.0.5",
    "@nx/devkit": "18.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.1.4",
    "@angular-devkit/core": "~17.1.4",
    "@angular-devkit/schematics": "~17.1.4",
    "@angular-eslint/eslint-plugin": "~17.0.1",
    "@angular-eslint/eslint-plugin-template": "~17.0.1",
    "@angular-eslint/template-parser": "~17.0.1",
    "@angular/cli": "~17.1.4",
    "@angular/compiler-cli": "~17.1.3",
    "@angular/language-service": "~17.1.3",
    "@nx/eslint": "18.0.5",
    "@nx/eslint-plugin": "18.0.5",
    "@nx/jest": "18.0.5",
    "@nx/js": "18.0.5",
    "@nx/plugin": "^18.0.7",
    "@nx/storybook": "18.0.5",
    "@nx/web": "18.0.5",
    "@nx/workspace": "18.0.5",
    "@release-it/bumper": "^6.0.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@schematics/angular": "~17.1.4",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/angular": "^7.6.17",
    "@storybook/core-server": "^7.6.17",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.8.0",
    "@swc/cli": "~0.1.65",
    "@swc/core": "~1.3.107",
    "@swc/helpers": "~0.5.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.18",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~13.1.6",
    "jsonc-eslint-parser": "^2.4.0",
    "ng-packagr": "~17.1.2",
    "nx": "18.0.5",
    "postcss": "^8.4.35",
    "postcss-url": "~10.1.3",
    "prettier": "^3.2.5",
    "prettier-eslint-cli": "^8.0.1",
    "release-it": "^17.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.1",
    "typescript": "~5.3.3"
  },
  "release-it": {
    "hooks": {
      "before:init": "pnpm before:release"
    },
    "npm": {
      "publish": false
    },
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagName": "${version}",
      "tagAnnotation": "Release ${version}"
    },
    "github": {
      "tokenRef": "GITHUB_NGUTIL_RELEASE_IT_TOKEN",
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "chore",
              "section": "Other changes"
            },
            {
              "type": "chore",
              "scope": "release",
              "hidden": true
            }
          ]
        },
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      },
      "@release-it/bumper": {
        "out": {
          "file": "packages/*/package.json"
        }
      }
    }
  }
}
