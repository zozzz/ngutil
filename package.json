{
  "name": "@ngutil/source",
  "version": "0.0.102",
  "scripts": {
    "before:release": "pnpm before:release:syncpack && pnpm before:release:update-lockfile && pnpm before:release:format",
    "before:release:format": "pnpm nx format:write && git add . && (git diff-index --quiet HEAD || git commit -m \"format: Beautify files\")",
    "before:release:syncpack": "pnpm syncpack fix-mismatches && pnpm syncpack format && git add . && (git diff-index --quiet HEAD || git commit -m \"format: Sync pacakge versions\")",
    "before:release:test": "pnpm nx run-many -t test",
    "before:release:update-lockfile": "pnpm i --lockfile-only && git add . && (git diff-index --quiet HEAD || git commit -m \"chore(tools): Update pnpm-lock.yaml\")",
    "build:libs": "pnpm nx run-many -t build --projects=packages/*",
    "release": "pnpm release-it --ci -V --increment=patch"
  },
  "dependencies": {
    "@angular/animations": "20.3.13",
    "@angular/cdk": "20.2.14",
    "@angular/common": "20.3.13",
    "@angular/compiler": "20.3.13",
    "@angular/core": "20.3.13",
    "@angular/forms": "20.3.13",
    "@angular/platform-browser": "20.3.13",
    "@angular/platform-browser-dynamic": "20.3.13",
    "@angular/router": "20.3.13",
    "@nx/angular": "22.1.1",
    "@nx/devkit": "22.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1",
    "storybook": "10.0.0",
    "tslib": "^2.6.2",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.9",
    "@angular-devkit/core": "20.3.9",
    "@angular-devkit/schematics": "20.3.9",
    "@angular-eslint/eslint-plugin": "20.3.0",
    "@angular-eslint/eslint-plugin-template": "20.3.0",
    "@angular-eslint/template-parser": "20.3.0",
    "@angular/cli": "~20.3.0",
    "@angular/compiler-cli": "20.3.13",
    "@angular/language-service": "20.3.13",
    "@eslint/config-inspector": "^0.4.7",
    "@nx/eslint": "22.1.1",
    "@nx/eslint-plugin": "22.1.1",
    "@nx/jest": "22.1.1",
    "@nx/js": "22.1.1",
    "@nx/plugin": "22.1.1",
    "@nx/storybook": "22.1.1",
    "@nx/web": "22.1.1",
    "@nx/workspace": "22.1.1",
    "@release-it/bumper": "^6.0.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@schematics/angular": "20.3.9",
    "@storybook/angular": "10.0.0",
    "@storybook/core-server": "8.6.11",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "0.22.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "1.9.2",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.13",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "29.5.13",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@typescript-eslint/utils": "^7.16.0",
    "autoprefixer": "^10.4.19",
    "eslint": "<9",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "^28.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "14.6.2",
    "jsonc-eslint-parser": "^2.4.0",
    "ng-packagr": "20.3.2",
    "nx": "22.1.1",
    "postcss": "^8.4.38",
    "postcss-url": "~10.1.3",
    "prettier": "^3.2.5",
    "prettier-eslint-cli": "^8.0.1",
    "release-it": "^17.2.1",
    "syncpack": "^12.3.2",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "license": "MIT",
  "peerDependencies": {
    "es-toolkit": "^1.35.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "utility-types": "^3.11.0",
    "zone.js": "0.15.0"
  },
  "private": true,
  "release-it": {
    "hooks": {
      "before:init": "pnpm before:release"
    },
    "npm": {
      "publish": false
    },
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagName": "${version}",
      "tagAnnotation": "Release ${version}"
    },
    "github": {
      "tokenRef": "GITHUB_NGUTIL_RELEASE_IT_TOKEN",
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "chore",
              "section": "Other changes"
            },
            {
              "type": "chore",
              "scope": "release",
              "hidden": true
            }
          ]
        },
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      },
      "@release-it/bumper": {
        "out": {
          "file": "packages/*/package.json"
        }
      }
    }
  }
}
