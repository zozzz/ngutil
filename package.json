{
  "name": "@ngutil/source",
  "version": "0.0.18",
  "license": "MIT",
  "scripts": {
    "before:release:update-packages": "pnpm update -w -r --latest && git add . && (git diff-index --quiet HEAD || git commit -m \"chore(tools): Update pnpm-lock.yaml\")",
    "before:release:test": "pnpm nx run-many -t test",
    "before:release:format": "pnpm nx format:write && git add . && (git diff-index --quiet HEAD || git commit -m \"format: Beautify files\")",
    "before:release": "pnpm before:release:format",
    "release": "pnpm release-it --ci -V --increment=patch",
    "build:libs": "pnpm nx run-many -t build --projects=packages/*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.6",
    "@angular/cdk": "^17.3.6",
    "@angular/common": "^17.3.6",
    "@angular/compiler": "^17.3.6",
    "@angular/core": "^17.3.6",
    "@angular/forms": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@angular/router": "^17.3.6",
    "@nx/angular": "18.3.4",
    "@nx/devkit": "18.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.3.6",
    "@angular-devkit/core": "~17.3.6",
    "@angular-devkit/schematics": "~17.3.6",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.3.6",
    "@angular/compiler-cli": "~17.3.6",
    "@angular/forms": "*",
    "@angular/language-service": "~17.3.6",
    "@eslint/config-inspector": "^0.4.7",
    "@nx/eslint": "18.3.4",
    "@nx/eslint-plugin": "18.3.4",
    "@nx/jest": "18.3.4",
    "@nx/js": "18.3.4",
    "@nx/plugin": "18.3.4",
    "@nx/storybook": "18.3.4",
    "@nx/web": "18.3.4",
    "@nx/workspace": "18.3.4",
    "@release-it/bumper": "^6.0.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@schematics/angular": "~17.3.6",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/angular": "^8.0.9",
    "@storybook/core-server": "^8.0.9",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.9.0",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.2",
    "@swc/helpers": "~0.5.11",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "autoprefixer": "^10.4.19",
    "eslint": "<9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "14.0.3",
    "jsonc-eslint-parser": "^2.4.0",
    "ng-packagr": "~17.3.0",
    "nx": "18.3.4",
    "postcss": "^8.4.38",
    "postcss-url": "~10.1.3",
    "prettier": "^3.2.5",
    "prettier-eslint-cli": "^8.0.1",
    "release-it": "^17.2.1",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "typescript": "~5.4.5"
  },
  "peerDependencies": {
    "@types/lodash": "^4.17.0",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "utility-types": "^3.11.0",
    "zone.js": "~0.14.4"
  },
  "release-it": {
    "hooks": {
      "before:init": "pnpm before:release"
    },
    "npm": {
      "publish": false
    },
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagName": "${version}",
      "tagAnnotation": "Release ${version}"
    },
    "github": {
      "tokenRef": "GITHUB_NGUTIL_RELEASE_IT_TOKEN",
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "chore",
              "section": "Other changes"
            },
            {
              "type": "chore",
              "scope": "release",
              "hidden": true
            }
          ]
        },
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      },
      "@release-it/bumper": {
        "out": {
          "file": "packages/*/package.json"
        }
      }
    }
  }
}
