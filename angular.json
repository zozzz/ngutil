{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "floating": {
      "projectType": "library",
      "root": "packages/floating",
      "sourceRoot": "packages/floating/src",
      "prefix": "nu",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/floating/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/floating/src/test.ts",
            "tsConfig": "packages/floating/tsconfig.spec.json",
            "karmaConfig": "packages/floating/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "packages/floating/**/*.ts",
              "packages/floating/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "floating/.storybook",
            "browserTarget": "floating:build",
            "compodoc": false
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "floating/.storybook",
            "browserTarget": "floating:build",
            "compodoc": false,
            "outputDir": "dist/storybook/floating"
          }
        }
      }
    }
  }
}
